+        SGL::logMessage(null, PEAR_LOG_DEBUG);
+        require_once(SGL_MOD_DIR . '/navigation2/classes/NaviTree.php');
+        $tree = & Navitree::singleton();
+        $output->test = $tree->renderHtmlList(1);
+        $output->test2 = $tree->renderHtmlList(2);
+    }
