+<?php
+
+define('SGL_NAV2_NODETYPE_EMPTY',   0);
+define('SGL_NAV2_NODETYPE_URI',     1);
+define('SGL_NAV2_NODETYPE_EXTURI',  2);
+define('SGL_NAV2_NODETYPE_LINK',    3);
+
+/**
+ * Base class for navigaion tree to be used with navigation etc
+ * Selects nested set from navinode table and build tree
+ * no pear module used to keep this light and fast
+ * TODO: Make class that extends this for editing navigation tree add, move, delete methods for nodes
+ * 
+ */
+class Navitree {
+    var $aRootNodes;
+    var $aNodes;
+    
+    function Navitree() {
+        if(empty($this->aNodes)) {
+            $this->_buildTree();
+        }
+    }
+    
+    function &singleton()
+    {
+        static $instance;
+        if (!isset($instance)) {
+            $instance = new Navitree();
+        }
+        return $instance;
+    }
+    
+    function _buildTree() {
+        $dbh = & SGL_DB::singleton();
+        $req = & SGL_Request::singleton();
+        $qs = addslashes($req->getUri());
+        $query = "
+        SELECT 
+            node.navinode_id, 
+            node.title,
+            node.type,
+            node.value,
+            node.cleanuri,
+            (COUNT(parent.navinode_id) - 1) AS depth,
+            MIN(parent.navinode_id) AS root_id,
+            (
+                (node.cleanuri IS NOT NULL AND POSITION(node.cleanuri IN '$qs') = 1) OR 
+                (node.type = 1 AND POSITION(node.value IN '$qs') = 1)
+            )
+                AS isCurrent
+        FROM navinode AS node,
+        navinode AS parent
+        WHERE node.lft BETWEEN parent.lft AND parent.rght
+        GROUP BY node.navinode_id
+        ORDER BY node.lft;
+        ";
+        $this->aNodes = array();
+        $this->aRootNodes = array();
+        // build tree
+        $aPS = array(); // to save refs to parents and determine max-depth
+        $prevNode = null;
+        $aRows = $dbh->getAssoc($query);
+        foreach($aRows as $key => $n) {
+            $node = new Navinode();
+            $node->setFrom($n);
+            $this->aNodes[$n->navinode_id] = & $node;
+            if($node->depth == 0) {
+                $this->aRootNodes[] = $node;
+            } else {
+                if($node->depth > $prevNode->depth) {
+                    // entering new level
+                    $prevNode->children = array();
+                    $aPS[$prevNode->depth] = $prevNode;
+                } else {
+                    // getting up in tree
+                }
+                $aPS[($node->depth - 1)]->children[] = $node;
+            }
+            if($node->isCurrent) {
+                for($i = 0; $i < $node->depth; $i++) {
+                    $aPS[$i]->childIsCurrent = true;
+                }
+            }
+            $prevNode = $node;
+            unset($node);
+        }
+        $this->maxDepth = count($aPS);
+    }
+    
+    /**
+      * @param rootId id of root of subtree to be rendered
+      */
+    function renderHtmlList($rootId = false, $aParams = false) {
+        if(!$aParams) {
+            // set default parameters
+            $aParams = array(
+                'collapsed' => false,
+            );
+        }
+        // select first root node if not given
+        if(!$rootId) {
+            if(isset($this->aRootNodes[0])) {
+                $rootId = $this->aRootNodes[0]->navinode_id;
+            } else {
+                return false;
+            }
+        }
+        $node = $this->aNodes[$rootId];
+        $html = '<ul>';
+        foreach($node->children as $subNode) {
+            $html .= '<li'.(($subNode->isCurrent || $subNode->childIsCurrent) ? ' class="current"':'').'>';
+            $html .= $subNode->toHtml();
+            if(isset($subNode->children)) {
+                if(!$aParams['collapsed'] || $subNode->childIsCurrent) {
+                    $html .= $this->renderHtmlList($subNode->navinode_id);
+                }
+            }
+            $html .= '</li>';
+        }
+        $html .= '</ul>';
+        return $html;
+    }
+    
+}
+
+class Navinode {
+    var $navinode_id = 0;
+    var $type = 0;
+    var $title = '';
+    var $value = 0;
+    var $is_enabled = 0;
+    var $cleanuri = '';
+    var $isCurrent = false;
+    var $childIsCurrent = false;
+    
+    function setFrom($obj) {
+        foreach (get_object_vars($obj) as $prop => $val) {
+            $this->$prop = $val;
+        }
+    }
+    /** get nodes html representation (with anchors, classes etc) */
+    function toHtml() {
+        $html = '';
+        if(empty($this->cleanuri)) {
+            // set link according to type
+            switch($this->type) {
+                case SGL_NAV2_NODETYPE_URI:
+                    $mod = '';
+                    $man = '';
+                    $pString = '';
+                    @ list($mod, $man, $pString) = explode('/',$this->value,3);
+                    $html = "<a href=\"".SGL_Output::makeUrl('',$man,$mod,array(),$pString)."\">$this->title</a>";
+                    break;
+                case SGL_NAV2_NODETYPE_EXTURI:
+                    $html = "<a href=\"$this->value\">$this->title</a>";
+                    break;
+                case SGL_NAV2_NODETYPE_LINK:
+                    //value has navinode_id to use as an alias
+                    $html = "<a href=\"$this->value\">$this->title</a>";
+                    break;            
+                case SGL_NAV2_NODETYPE_EMPTY:
+                default:
+                    $html = "<a href=\"#\">{$this->title}</a>";
+                    break;
+            }
+        } else {
+            // doesn't look quality code, but ain't gonna renew everything :(
+            $html = "<a href=\"".SGL_Output::makeUrl('',$this->cleanuri,$this->cleanuri)."\">{$this->title}</a>";
+        }
+        return $html;
+    }
+}
+?>

